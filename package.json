{
  "name": "discord-kommando.js",
  "version": "2.2.0",
  "description": "An automatic interaction handler for discord.js",
  "main": "dist/main.js",
  "exports": {
    ".": {
      "require": "./dist/main.js",
      "import": "./v14.mjs",
      "types": "./dist/main.d.ts"
    },
    "./v14": {
      "require": "./dist/structures/v14/index.js",
      "import": "./v14.mjs",
      "types": "./dist/structures/v14/index.d.ts"
    },
    "./v14.js": {
      "require": "./dist/structures/v14/index.js",
      "import": "./v14.mjs",
      "types": "./dist/structures/v14/index.d.ts"
    }
  },
  "scripts": {
    "test": "echo **Error** No test specified.",
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "docs": "typedoc src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team-ThinkCord/discord-kommando.js.git"
  },
  "keywords": [
    "discord.js",
    "commandhandler"
  ],
  "author": "Icetang0123, Feven7",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Team-ThinkCord/discord-kommando.js/issues"
  },
  "homepage": "https://github.com/Team-ThinkCord/discord-kommando.js#readme",
  "dependencies": {
    "discord-api-types": "^0.37.24",
    "discord.js-14": "npm:discord.js@^14.11.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^17.0.30",
    "discord.js": "^14.11.0",
    "typedoc": "^0.23.28",
    "typescript": "^4.8.4"
  }
}
